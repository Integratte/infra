{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:/logs/Infra.TestesDeApi/log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }

}
